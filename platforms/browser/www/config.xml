<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bazarsoo.app" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <gap:plugin name="de.appplant.cordova.plugin.local-notification" />
    <feature name="BackgroundMode">
        <param name="browser-package" value="BackgroundMode" />
    </feature>
    <name>Bazarsoo</name>
    <description>
    </description>
    <author email="info@bazarsoo.com" href="http://bazarsoo.com">
    Bazarsoo
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
